<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.talker.processor"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="8" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >


        <activity android:name=".SplashScreen"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UBTalkerActivity">
            <intent-filter>
                <action android:name="com.talker.processor.UBTalkerActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity class=".TaskSelection" android:label="TaskSelection" android:name="TaskSelection">
        </activity>
        <activity class=".TaskSelectionAdmin" android:label="TaskSelectionAdmin" android:name="TaskSelectionAdmin">
        </activity>
        <activity class=".PictureDB" android:label="PictureDB" android:name="PictureDB">
        </activity>
        <activity class=".BookReader" android:label="BookReader" android:name="BookReader">
        </activity>
        <activity class=".phrase" android:label="phrase" android:name="com.talker.processor.phrase">
        </activity>
        </application>

</manifest>